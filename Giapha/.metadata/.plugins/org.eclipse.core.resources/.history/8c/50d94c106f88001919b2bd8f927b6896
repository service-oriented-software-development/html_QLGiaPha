package jsoft.ads.individual;

import java.sql.ResultSet;

import Common.ShareControl;
import jsoft.ads.object.MemberObject;

public interface Member extends ShareControl{
	public boolean addFamilyTree(MemberObject item);
	public boolean editFamilyTree(MemberObject item);
	public boolean delFamilyTree(MemberObject item);
	
	public ResultSet getChild(int id);
	public ResultSet getFamilyTree(int id);
	public ResultSet getFamilyTrees();
	public ResultSet getFamilyTrees(MemberObject similar, int at, byte total);
}
